# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置 (MongoDB Atlas 连接串)
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/tcm_db?retryWrites=true&w=majority
